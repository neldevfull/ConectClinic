<% @users.each do |user| %>
	<tr>								
		<th>
			<%=
				link_to user.name,
					edit_user_path(user),
					class: ""  											 
			%></th>
		<th><%= user.email %></th>
		<th>
			<%= 
				if user.career == "healthcare"
					"Profissional da saude"
				else
					"Atendimento"
				end
			%>
		</th>		
		<th><%= user.status%></th>
	</tr>
<% end %>