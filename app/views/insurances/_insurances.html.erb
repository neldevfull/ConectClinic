<% @insurances.each do |insurance| %>
	<tr>								
		<th>
			<%= insurance.name %></th>
		<th><%=
				link_to insurance.identifier,
					edit_insurance_path(insurance),
					class: ""
			%></th>
		<th><%= insurance.city %></th>
		<th><%= insurance.state %></th>		
		<th><%= insurance.status %></th>
	</tr>
<% end %>