<tr>								
	<th>
		<%=
			link_to patient.name,
				edit_patient_path(patient),
				class: ""  											 
		%></th>
	<th><%= patient.email %></th>
	<th>
		<%= 
			if patient.cellphone.present?
				patient.cellphone
			else
				patient.telephone
			end 
		%>
	</th>		
	<th>-----</th>							
	<th>-----</th>
</tr>