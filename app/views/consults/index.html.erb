							<div id="calendar"></div>
							<div id="dialog" title="Agendar Consulta">
							    <div id="dialog-inner">
							    	
							    		<div class="form-group">							    		
									    	<label class="control-label _sl col-sm-2 col-lg-2">Nome</label>
									    	<div class="col-sm-8 col-lg-8">												
												<input type="text" class="form-control" id="name">						
											</div>
										</div>
										<div class="form-group">							    		
									    	<label class="control-label _sl col-sm-2 col-lg-2">E-mail</label>
