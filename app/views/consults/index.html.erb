									    	<label class="control-label _sl col-sm-2 col-lg-2">Horario</label>
									    	<div class="col-sm-8 col-lg-8">												
												<input type="text" class="form-control">
											</div>											
										</div>									
							    </div>
							</div>
						</div>
					</div>														
